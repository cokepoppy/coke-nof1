# Database
DB_ROOT_PASSWORD=your_root_password
DB_USER=nof1_user
DB_PASSWORD=your_db_password
DB_NAME=nof1_db
DB_HOST=mysql
DB_PORT=3306

# JWT
JWT_SECRET=your_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=7d

# LLM API Keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
GOOGLE_AI_API_KEY=...
XAI_API_KEY=...
DEEPSEEK_API_KEY=...
QWEN_API_KEY=...

# OpenRouter API (unified access to all models)
OPENROUTER_API_KEY=sk-or-v1-...

# Exchange API (Hyperliquid)
HYPERLIQUID_API_KEY=...
HYPERLIQUID_SECRET=...
HYPERLIQUID_TESTNET=true

# Binance US API (for market data)
BINANCE_API_KEY=...
BINANCE_API_SECRET=...

# Application
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:5173
CORS_ORIGIN=http://localhost:5173

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Trading Configuration
TRADING_INTERVAL_MS=86400000  # 1 day = 86400000ms, 3 minutes = 180000ms
MAX_LEVERAGE=20
MAX_POSITION_RISK=0.03
ENABLE_REAL_TRADING=false  # Set to true for real trading
ENABLE_AI_TRADING=false  # Set to true to enable AI automated trading

# Network Proxy (optional, for network access issues)
HTTP_PROXY=http://172.25.64.1:7890
HTTPS_PROXY=http://172.25.64.1:7890

# CoinGecko API Configuration
COINGECKO_POLL_INTERVAL_MS=60000  # 60 seconds (免费API限制，建议60秒或更长)
